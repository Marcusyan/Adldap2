{
    "name": "marcusyan/adldap2",
    "type": "library",
    "description": "A PHP LDAP Package for humans.",
    "keywords": [
        "active directory",
        "directory",
        "ad",
        "ldap",
        "windows",
        "adldap",
        "adldap2"
    ],
    "require": {
        "php": ">=7.0",
        "ext-ldap": "*",
        "ext-json": "*",
        "psr/log": "*",
        "psr/simple-cache": "*",
        "tightenco/collect": "*",
        "illuminate/contracts": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "mockery/mockery": "*"
    },
    "suggest": {
        "ext-fileinfo": "fileinfo is required when retrieving user encoded thumbnails"
    },
    "archive": {
        "exclude": ["/examples", "/tests"]
    },
    "autoload": {
        "psr-4": {
            "Adldap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adldap\\Tests\\": "tests/"
        }
    }
}
